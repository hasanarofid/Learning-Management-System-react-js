{
  "name": "lms-system",
  "version": "1.0.0",
  "description": "Learning Management System dengan React.js dan MySQL",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "setup": "npm install && cd server && npm install && cd ..",
    "db:setup": "mysql -u root -phasanitki -e \"CREATE DATABASE IF NOT EXISTS db_lis;\" && mysql -u root -phasanitki db_lis < database/schema.sql",
    "start:all": "./start.sh"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.8.0",
    "axios": "^1.3.0",
    "styled-components": "^5.3.6",
    "react-icons": "^4.7.1",
    "framer-motion": "^10.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}